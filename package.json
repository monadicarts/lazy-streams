{
  "name": "@monadica/lazy-streams",
  "version": "1.1.0",
  "source": "./src/index.ts",
  "main": "./dist/main.js",
  "types": "./dist/types.d.ts",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "docs",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "unpkg": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "packageManager": "npm@10.9.0",
  "exports": {
    ".": {
      "module": "./dist/main.js",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/main.js"
      },
      "require": {
        "types": "./dist/types.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/main.js"
    },
    "./package.json": "./package.json"
  },  
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monadicarts/lazy-streams.git"
  },
  "scripts": {
    "serve": "parcel docs/index.html --open firefox",
    "watch": "parcel watch",
    "prebuild": "npx rimraf dist && mkdir dist",
    "build": "parcel build src/index.ts --dist-dir dist --no-content-hash",
    "prepare": "npm run build",
    "precompress": "rm dist/template-ts-npm.*",
    "compress": "tar -czvf template-ts-npm.tar.gz dist && zip -r template-ts-npm.zip dist && mv template-ts-npm.* dist/",
    "test": "jest",
    "docs": "typedoc",
    "postdocs": "npm run format",
    "lint": "eslint --fix .",
    "postlint": "npm run format",
    "format": "prettier --write .",
    "release": "standard-version",
    "publish:public": "npm publish --access public"
  },
  "keywords": [
    "typescript",
    "module",
    "library",
    "open-source"
  ],
  "author": "Monadica <monadicarts@gmail.com>",
  "license": "MIT",
  "description": "Lazy data stream processing in TypeScript.",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@parcel/bundler-library": "^2.12.0",
    "@parcel/compressor-brotli": "^2.13.0",
    "@parcel/compressor-gzip": "^2.13.0",
    "@parcel/optimizer-cssnano": "^2.13.0",
    "@parcel/optimizer-terser": "^2.13.0",
    "@parcel/packager-ts": "^2.13.0",
    "@parcel/transformer-babel": "^2.13.0",
    "@parcel/transformer-typescript-tsc": "^2.13.0",
    "@parcel/transformer-typescript-types": "^2.13.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "parcel": "^2.13.0",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "targets": {
    "main": {
      "distDir": "dist",
      "context": "node",
      "isLibrary": true,
      "scopeHoist": true,
      "sourceMap": true,
      "engines": {
        "node": ">=23.0.0 <24.0.0"
      }
    },
    "module": {
      "distDir": "dist",
      "context": "node",
      "isLibrary": true,
      "scopeHoist": true,
      "sourceMap": true,
      "engines": {
        "node": ">=23.0.0 <24.0.0"
      }
    },
    "browser": {
      "distDir": "dist",
      "context": "browser",
      "isLibrary": true,
      "scopeHoist": true,
      "sourceMap": {
        "inline": true
      },
      "engines": {
        "browsers": "> 0.25%"
      }
    }
  },
  "browserslist": "> 0.2%, last 2 versions, not dead",

  "directories": {
    "doc": "docs"
  },
  "bugs": {
    "url": "https://github.com/monadicarts/lazy-streams/issues"
  },
  "homepage": "https://github.com/monadicarts/lazy-streams#readme"
}
