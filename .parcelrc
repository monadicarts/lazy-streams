{
  "extends": "@parcel/config-default",
  "bundler": "@parcel/bundler-library",
  "transformers": {
    "*.{ts,tsx}": [
      "@parcel/transformer-typescript-tsc",
      "@parcel/transformer-babel"
    ]
  },
  "optimizers": {
    "*.js": [
      "@parcel/optimizer-terser"
    ],
    "*.css": [
      "@parcel/optimizer-cssnano"
    ]
  }
}